{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "images/**", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/$", "dest": "/index.html" },
    { "src": "^/menu$", "dest": "/Menu.html" },
    { "src": "^/tentangkami$", "dest": "/TentangKami.html" },
    { "src": "^/pesanan$", "dest": "/Pesanan.html" },
    { "src": "^/pembayaran$", "dest": "/pembayaran.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
